{
  "private": true,
  "license": "MIT",
  "packageManager": "pnpm@7.13.2",
  "scripts": {
    "dev": "nuxt dev -p 3333",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "prepare": "is-ci || husky install",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "typecheck": "nuxi typecheck",
    "run:eslint": "eslint --fix --cache --cache-location ./node_modules/.cache/eslint/ --ext .ts,.vue,.json,.yaml,.yml .",
    "run:stylelint": "stylelint --fix --cache --cache-location ./node_modules/.cache/stylelint/ '**/*.{css,scss,vue}'",
    "lint": "pnpm run:eslint && pnpm run:stylelint",
    "format": "prettier -uw --cache ."
  },
  "dependencies": {
    "@pinia/nuxt": "0.4.2"
  },
  "devDependencies": {
    "@nuxt/test-utils-edge": "3.0.0-rc.12-27730995.2894a75",
    "@testing-library/vue": "6.6.1",
    "@typescript-eslint/eslint-plugin": "5.38.0",
    "@typescript-eslint/parser": "5.38.0",
    "@unocss/nuxt": "0.45.22",
    "@vitest/coverage-c8": "0.23.4",
    "@vueuse/core": "9.2.0",
    "@vueuse/nuxt": "9.2.0",
    "eslint": "8.24.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jsonc": "2.4.0",
    "eslint-plugin-vue": "9.5.1",
    "eslint-plugin-yml": "1.2.0",
    "happy-dom": "6.0.4",
    "husky": "8.0.1",
    "is-ci": "3.0.1",
    "lint-staged": "13.0.3",
    "msw": "0.47.3",
    "nuxt": "3.0.0-rc.13",
    "nuxt-icon": "^0.1.6",
    "postcss-html": "1.5.0",
    "prettier": "2.7.1",
    "sass": "1.55.0",
    "stylelint": "14.12.1",
    "stylelint-config-css-modules": "4.1.0",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-recess-order": "3.0.0",
    "stylelint-config-standard-scss": "5.0.0",
    "stylelint-config-standard-vue": "1.0.0",
    "typescript": "4.8.4",
    "vitest": "0.23.4",
    "vue-tsc": "0.40.13"
  },
  "lint-staged": {
    "*.{ts,vue,json,yaml,yml}": "eslint --fix --cache --cache-location ./node_modules/.cache/eslint/",
    "*.{css,scss,vue}": "stylelint --fix --cache --cache-location ./node_modules/.cache/stylelint/",
    "*": "prettier -wu --cache",
    "*.{test,spec}.ts": "vitest related"
  }
}
